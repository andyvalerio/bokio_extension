{
    "permissions": ["scripting"],
    "name": "VAT inclusive price in Bokio",
    "manifest_version": 3,
    "host_permissions": ["http:\/\/*\/*", "https:\/\/*\/*"],
    "description": "Show the price including VAT in Bokio",
    "content_scripts": [
        {
          "matches": ["*://app.bokio.se/*"],
          "js": ["contentScript.js"]
        }
    ],
    "version": "1.0"
}